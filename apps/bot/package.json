{
  "name": "@otr-discord-bot/bot",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "scripts": {
    "start": "bun run .",
    "dev": "bun --watch run .",
    "deploy": "bun run ./src/discord/scripts/deploy-commands.ts --global",
