{
  "name": "otr-bot",
  "version": "1.0.0",
  "description": "A discord bot for interacting with osu! Tournament Rating",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run ./src/index.ts",
    "dev": "bun --watch run ./src/index.ts",
    "deploy": "bun run ./src/discord/scripts/deploy-commands.ts --global",
    "deploy:dev": "bun run ./src//discord/scripts/deploy-commands.ts",
    "clear": "bun run ./src//discord/scripts/clear-commands.ts --global",
    "clear:dev": "bun run ./src//discord/scripts/clear-commands.ts",
    "lint": "bunx eslint --fix",
    "format": "bunx prettier --write ."
  },
  "author": "myssto",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "^1.3.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
